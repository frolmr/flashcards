<%= simple_form_for :session, url: user_sessions_path do |f| %>
  <%= f.input :email, label: 'Email' %>
  <%= f.input :password, label: 'Пароль' %>
  <%= f.submit 'Войти', class: 'btn btn-primary' %>
<% end %>
<br><br>
<%= link_to 'Login with Twitter', auth_at_provider_path(:provider => :twitter) %> <br>
<%= link_to 'Login with Facebook', auth_at_provider_path(:provider => :facebook) %> <br>
<%= link_to 'Login with VK', auth_at_provider_path(:provider => :vk) %>